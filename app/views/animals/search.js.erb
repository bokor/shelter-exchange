<% unless @animals.blank? %>
	$('#animals').html( "<%= escape_javascript( render 'animal_list', :animals => @animals) %>" );
<% else %>
	$('#animals').html( "<%= escape_javascript( render 'no_animals' ) %>" );
<% end %>

// Google Anayltics
<% if Rails.env.production? %>
  _gaq.push(['_trackPageview', "/animals/search"]);
<% end %>

