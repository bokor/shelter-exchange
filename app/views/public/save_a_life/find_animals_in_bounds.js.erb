<% unless @animals.blank? %>
	$('#map_animals').html( "<%= escape_javascript( render 'animal_list', :animals => @animals) %>" );
<% else %>
	if ($('#search_not_found').length == 0) {
		$('#map_animals').html("<%= escape_javascript( render 'not_found_error') %>");
  }
<% end %>

// Google Analytics
<% if Rails.env.production? %>
  _gaq.push(['_trackPageview', "/save_a_life/find_animals_on_map"]);
<% end %>

