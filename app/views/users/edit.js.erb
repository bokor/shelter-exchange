$('#user_<%=@user.id%>').hide();
$('#user_<%=@user.id%>').after("<%= escape_javascript(render 'edit_user') %>");
$('#edit_user_<%=@user.id%>').show();

// Google Analytics
<% if Rails.env.production? %>
  _gaq.push(['_trackPageview', "/users/{id}/edit"]);
<% end %>

// Discard Flash Message
<% flash.discard %>

